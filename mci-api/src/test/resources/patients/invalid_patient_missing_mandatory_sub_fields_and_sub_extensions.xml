<Patient xmlns="http://hl7.org/fhir">
    <extension url="https://sharedhealth.atlassian.net/wiki/display/docs/fhir-extensions/BloodGroup">
        <valueString value="A"/>
    </extension>
    <extension url="https://sharedhealth.atlassian.net/wiki/display/docs/fhir-extensions/confidentiality">
        <valueBoolean value="true"/>
    </extension>
    <name>
        <use value="usual"/>
    </name>
    <gender value="male"/>

    <birthDate value="1976-01-12">
        <extension url="http://hl7.org/fhir/StructureDefinition/patient-birthTime">
            <valueDateTime value="1976-01-12T16:50:00+05:30"/>
        </extension>
    </birthDate>
    <address>
        <use value="home"/>
    </address>
</Patient>